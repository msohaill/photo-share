{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["dom","es2017"],
    "outDir": "app",
    "rootDir": "./",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true
  }
}
